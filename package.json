{
  "name": "press",
  "description": "Semantic release setup for the press project.",
  "repository": {
    "type": "git",
    "url": "https://github.com/digikwal/press.git"
  },
  "scripts": {
    "test": "echo \"No tests defined\" && exit 0"
  },
  "dependencies": {
    "semantic-release": "^24.2.3"
  },
  "release": {
    "branches": [
      "main",
      "dev"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
